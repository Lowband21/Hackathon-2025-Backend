# .env.example - Example environment variables
# Copy this file to .env and fill in your actual values.
# DO NOT COMMIT THE ACTUAL .env FILE.

# Django
DJANGO_SECRET_KEY=YOUR_DJANGO_SECRET_KEY_HERE # Replace with a real secret key in .env
DEBUG=True
DJANGO_ALLOWED_HOSTS=localhost 127.0.0.1 backend

# PostgreSQL
POSTGRES_DB=campus_serendipity_db
POSTGRES_USER=serendipity_user
POSTGRES_PASSWORD=YOUR_DB_PASSWORD_HERE # Replace with a real password in .env
POSTGRES_HOST=db
POSTGRES_PORT=5432